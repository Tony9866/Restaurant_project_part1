@using Restaurant_project.Models
@model IEnumerable<Genre>

@{
    ViewBag.Title = "Menu";
}

<h3>Browse Genres</h3>


<ul>
    @foreach (Genre item in Model)
    {
        //their equal to each other.
        //<li><a href="/Store/Browse?genre=@genre.Name">@genre.Name</a></li> 
        <li>@Html.ActionLink(item.Name, "Browse", new {genre= item.Name})</li>
    }
</ul>

